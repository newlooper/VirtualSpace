<Application x:Class="Cube3D.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:interop="clr-namespace:System.Windows.Interop;assembly=PresentationCore"
             StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <interop:D3DImage x:Key="Front" />
            <interop:D3DImage x:Key="Others" />
            <LinearGradientBrush x:Key="BackgroundLgb" StartPoint='0.5,0' EndPoint='0.5,1'>
                <GradientStop Color='Black' Offset='0' />
                <GradientStop Color='#FF323334' Offset='1' />
            </LinearGradientBrush>
            <Brush x:Key="BackgroundTrans">
                Transparent
            </Brush>
        </ResourceDictionary>
    </Application.Resources>

</Application>